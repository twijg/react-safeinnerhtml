{
  "name": "react-safeinnerhtml",
  "version": "0.3.0",
  "description": "React Component to safely use HTML in the DOM",
  "keywords": [
    "react",
    "react-component",
    "html",
    "react-dom",
    "dom",
    "javascript"
  ],
  "author": "Twijg IT B.V. <info@twijg-it.nl>",
  "contributors": [
    "Wiebe Cnossen <wiebe@twijg-it.nl>",
    "Jelle de Jong <jelle@twijg-it.nl>"
  ],
  "main": "dist/index.tsx",
  "license": "MIT",
  "scripts": {
    "test": "jest --roots src",
    "lint": "eslint -c .eslintrc.yml src/**/*.ts*",
    "prettier": "prettier --write src/**/*.ts*",
    "build": "cross-env NODE_ENV=production babel src/ --ignore **/*.test.js --out-dir dist/",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier --write",
      "eslint -c .eslintrc.yml",
      "git add"
    ]
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "htmlparser2": "^8.0.1",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.18.6",
    "@cfaester/enzyme-adapter-react-18": "^0.6.0",
    "@types/classnames": "^2.3.1",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^29.2.4",
    "@types/lodash": "^4.14.191",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "eslint": "^6",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-lodash-fp": "^2.1.3",
    "eslint-plugin-prettier": "^3",
    "eslint-plugin-react": "^7",
    "import-sort-style-module": "^6.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "jsdom": "^20.0.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.1.3",
    "pre-commit": "^1",
    "prettier": "^2.8.1",
    "prettier-plugin-import-sort": "^0.0.7",
    "regenerator-runtime": "^0.13.0"
  },
  "resolutions": {
    "braces": ">=3.0.2",
    "jsdom@^15.2.1": "^16.5.0",
    "node-notifier@^6.0.0": "^8.0.1",
    "nth-check@~1.0.1": "^2.0.1"
  },
  "homepage": "https://github.com/twijg/react-safeinnerhtml",
  "repository": {
    "url": "https://github.com/twijg/react-safeinnerhtml",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/twijg/react-safeinnerhtml/issues"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "prettier": {
    "endOfLine": "auto"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module"
    }
  }
}
